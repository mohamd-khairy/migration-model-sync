{
  "name": "khairy/migration-model-sync",
  "description": "Automatically sync Laravel models from migration files, including fillable, casts, and relationships.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "Laravel"
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "nikic/php-parser": "^4.13 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Khairy\\MigrationModelSync\\": "src/"
    }
  },
  "authors": [
    {
      "name": "mohamed khairy",
      "email": "mohamed.khairy.eg@gmail.com"
    }
  ],
  "homepage": "https://github.com/mohamd-khairy/migration-model-sync",
  "extra": {
    "laravel": {
      "providers": [
        "Khairy\\MigrationModelSync\\MigrationModelSyncServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
